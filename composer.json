{
    "name": "ruudk/payment-mollie-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony2 Bundle that provides access to the Mollie API. Based on JMSPaymentCoreBundle.",
    "keywords": ["payment", "mollie"],
    "homepage": "https://github.com/ruudk/PaymentMollieBundle",
    "license": "Apache2",
    "authors": [
        {
            "name": "Ruud Kamphuis",
            "email": "ruudk@mphuis.com"
        }
    ],
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/ruudk/mollie"
    }],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.0",
        "jms/payment-core-bundle": "~1.0",
        "psr/log": "~1.0",
        "omnipay/mollie": "dev-new-api"
    },
    "autoload": {
        "psr-0": { "Ruudk\\Payment\\MollieBundle": "" }
    },
    "target-dir": "Ruudk/Payment/MollieBundle"
}